    "            'Pendulum-v0']\n",
    "\n",
    "example = 0\n",
    "env = gym.make(name_map[example])\n",
    "env.reset()\n",
    "\n",
    "for _ in range(1000):\n",
    "    env.render()\n",
    "    action = env.action_space.sample()\n",
    "    env.step(action) \n",
    "\n",
    "env.close()"
